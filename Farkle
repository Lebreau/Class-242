class Player{
    constructor(player){
        this.player = player
}

class Dice {
constructor(sides){
        this._sides = sides;
    }

class Score {
     constructor(score)
        this.score = score;
        this.totalScore = 0;
        this.currentTurnScore = 0;
        this.Endscore = false;
    }
    
    
 class Rules {
  constructor(rules){
        this.rollSets = [];
        this.rules = rules;
        this.isFarkle = false;
    
 }
